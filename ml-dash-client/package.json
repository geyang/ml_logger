    "units-css": "latest",
    "window": "^4.2.6"
  },
  "scripts": {
    "start": "PORT=3001 react-scripts start",
    "start-backend": "source activate ml-logger && python -m ml_dash.main --logdir='../runs' --workers 4 --port=8081",
    "create-example-experiments": "source activate ml-lopgger && cd ../ && python ml-dash-server/tests/create_experiments.py",
    "generate-schema": "source activate ml-logger && python scripts/generate_schema.py",
    "relay": "relay-compiler --src ./src --schema ./schema.graphql --extensions js jsx",
    "refresh": "yarn generate-schema && yarn relay",
    "build": "react-scripts build",
    "deploy": "rm -rf ../ml-dash-server/ml_dash/client-dist && mv build ../ml-dash-server/ml_dash/client-dist",
    "build-deploy": "yarn build && yarn deploy",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "babel-plugin-relay": "^2.0.0-rc.2",
    "graphql": "^14.1.1",
    "lodash.tofinite": "^4.12.1",
    "relay-compiler": "^2.0.0"
  }
}
